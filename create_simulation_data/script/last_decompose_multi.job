#!/bin/bash
#$ -S /bin/bash
#$ -N DecMult
#$ -cwd
#$ -pe smp 24
#$ -e ./logfiles/last_decompose_multi_vb_prior.log
#$ -o ./logfiles/last_decompose_multi_vb_prior.out
#$ -V
#$ -m e
set -ue

OUTPUT=./result/last_decompose_multi_vb_prior.txt
OUTLOG_PREFIX=./logfiles/last_decompose_multi_vb_prior

TEST_NUM=300
COVERAGE=0
PROBS="0.16 0.16 0.16 0.16 0.16 0.16"
for seed in 10 20 30 40 50
do
    ./target/release/test_last_decompose_multiple \
        ${TEST_NUM} ${COVERAGE} ${seed} ${PROBS} >> ${OUTPUT}\
        2>> ${OUTLOG_PREFIX}.even.300.log
done

TEST_NUM=300
COVERAGE=0
PROBS="0.4 0.1 0.1 0.1 0.2 0.1"
for seed in 10 20 30 40 50
do
    ./target/release/test_last_decompose_multiple \
        ${TEST_NUM} ${COVERAGE} ${seed} ${PROBS} >> ${OUTPUT} \
        2>> ${OUTLOG_PREFIX}.skew.300.log
done

